{
  "name": "Sub Rosa",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Holds tricky words in a DB",
  "background" : {
    "scripts": ["js/jquery-1.8.3.js", "store.js"] 
    },
  "browser_action": {
    "default_title" : "Subrosa Dictionary",
    "default_popup": "popup.html"
    },
  "permissions" : [
    "contextMenus",
    "*://*.quizlet.com/*",
    "*://*.dictionaryapi.com/api/*",
    "*://*.google.com/dictionary/*"
    ]
}
