{
  "name": "Sub Rosa",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Holds tricky words in a DB",
  "background" : {
    "page": "background.html"
    },
  "content_scripts": [ {
    "all_frames": true,
    "js": [ "css/frames.css", "js/jquery-1.8.3.js", "store.js" ],
    "matches": [ "http://*/*", "https://*/*" ],
    "run_at": "document_end"
  } ],
  "browser_action": {
    "default_title" : "Subrosa Dictionary",
    "default_popup": "words.html"
    },
  "options_page": "words.html",
  "permissions" : [
    "tabs",
    "contextMenus",
    "*://*.dictionaryapi.com/api/*"
    ],
  "web_accessible_resources": [ "css/frames.css", "js/jquery-1.8.3.js"]
}
