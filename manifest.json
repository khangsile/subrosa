{
  "name": "Sub Rosa",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Holds tricky words in a DB",
  "background" : {
    "page": "background.html"
    },
  "browser_action": {
    "default_title" : "Subrosa Dictionary",
    "default_popup": "popup.html"
    },
  "options_page": "words.html",
  "permissions" : [
    "tabs",
    "contextMenus",
    "*://*.dictionaryapi.com/api/*"
    ]
}
